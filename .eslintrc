{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "extends": [
    "airbnb"
  ],
  "globals": {
    "it": true
  },
  "rules": {
    "react/jsx-filename-extension": [ 1, { "extensions": [".js", ".jsx"] } ],
    "linebreak-style": 0,
    "react/forbid-prop-types": 0
  }
}